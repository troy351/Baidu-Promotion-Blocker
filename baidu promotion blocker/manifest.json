{
	"name": "Baidu Promotion Blocker",
	"version": "1.0.0",
	"manifest_version":2,
	"description": "屏蔽百度搜索结果中的推广链接",
	"background": {
	    "scripts": ["background.js"]
    },
	"page_action": {
		"default_icon": {
			"19": "icon_19.png",
			"38": "icon_38.png"
		}
	},
	"content_scripts": [{
		"matches": ["*://*.baidu.com/*"],
		"js": ["jquery.js", "content_script.js"]
    }],
	"permissions": [
		"tabs"
	]
}